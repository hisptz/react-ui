{
  "name": "@hisptz/react-ui",
  "homepage": "https://hisptz.github.io/react-ui",
  "version": "1.0.39",
  "description": "A collection of reusable complex DHIS2 react ui components.",
  "license": "BSD-3-Clause",
  "scripts": {
    "build:types": "tsc --emitDeclarationOnly --declaration --outDir ./build/types",
    "build:package": "d2-app-scripts build",
    "build": "concurrently -n build,types \"yarn build:package\" \"yarn build:types\" && cp postinstall.js build/",
    "start": "d2-app-scripts start",
    "test:capture": "cypress run-ct --browser electron --env networkMode=capture",
    "test": "cypress run-ct --browser electron --env networkMode=stub",
    "cy:run": "cypress run --browser electron",
    "deploy": "d2-app-scripts deploy",
    "lint": "yarn d2-style check",
    "lint:staged": "yarn lint --staged",
    "format": "yarn d2-style apply",
    "format:staged": "yarn format --staged",
    "sb": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "cy:open": "yarn cypress open-ct --browser electron",
    "cy:open:stub": "yarn cypress open-ct --browser electron --env networkMode=stub",
    "predeploy": "yarn build-storybook",
    "deploy-storybook": "gh-pages -d storybook-static",
    "postinstall": "node postinstall.js",
    "install": "node postinstall.js"
  },
  "devDependencies": {
    "@cypress/react": "^5.12.3",
    "@cypress/webpack-dev-server": "^1.8.1",
    "@dhis2/app-runtime": "^3.2.5",
    "@dhis2/cli-app-scripts": "^8.2.3",
    "@dhis2/cli-style": "^9.3.1",
    "@dhis2/cli-utils-cypress": "^8.0.4",
    "@dhis2/cypress-commands": "^8.0.4",
    "@dhis2/cypress-plugins": "^8.0.4",
    "@dhis2/ui": "^7.14.2",
    "@hookform/devtools": "^4.0.2",
    "@storybook/addon-actions": "^6.4.19",
    "@storybook/addon-controls": "^6.4.19",
    "@storybook/addon-essentials": "^6.4.19",
    "@storybook/addon-links": "^6.4.19",
    "@storybook/node-logger": "^6.4.19",
    "@storybook/preset-create-react-app": "^3.2.0",
    "@storybook/react": "^6.4.19",
    "@types/jest": "^27.0.2",
    "@types/lodash": "^4.14.176",
    "@types/node": "^16.11.3",
    "@types/react-beautiful-dnd": "^13.1.2",
    "@types/react-color": "^3.0.6",
    "@types/react-dom": "^17.0.10",
    "@types/react-router-dom": "^5.3.2",
    "concurrently": "^6.4.0",
    "cypress": "^9.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "prettier": "^2.4.1",
    "react": "16.14.0",
    "react-scripts": "^4.0.3",
    "typescript": "^4.4.4",
    "wait-on": "^6.0.0",
    "webpack-dev-server": "^4.3.1"
  },
  "dependencies": {
    "@iapps/period-utilities": "^1.0.0-beta.5",
    "@material-ui/core": "^4.12.3",
    "@types/react": "16.14.0",
    "highcharts": "^9.3.0",
    "highcharts-react-official": "^3.1.0",
    "jodit-react": "^1.1.10",
    "print-message": "^3.0.1",
    "react-beautiful-dnd": "^13.1.0",
    "react-color": "^2.19.3",
    "react-hook-form": "^7.25.1"
  },
  "main": "./build/cjs/index.js",
  "module": "./build/es/index.js",
  "types": "./build/types/index.d.ts",
  "exports": {
    "import": "./build/es/index.js",
    "require": "./build/cjs/index.js"
  },
  "files": [
    "build/**",
    "postinstall.js"
  ],
  "peerDependencies": {
    "@dhis2/app-runtime": "^3.2.5",
    "@dhis2/ui": "^7.7.5",
    "lodash": "^4.17.21",
    "react": "^16.14.0"
  },
  "eslintConfig": {
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hisptz/react-ui.git"
  },
  "keywords": [
    "react",
    "dhis2",
    "dhis2-ui",
    "hisptz"
  ],
  "author": "HISP Tanzania",
  "bugs": {
    "url": "https://github.com/hisptz/react-ui/issues"
  }
}
